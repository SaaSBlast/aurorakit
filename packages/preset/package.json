{
  "name": "@aurorakit/preset",
  "description": "The preset for PandaCSS that contains the theme for AuroraKit",
  "version": "0.7.1",
  "keywords": [
    "pandacss",
    "shadcn",
    "radix"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "homepage": "https://aurorakit.saasblast.dev",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saasblast/aurorakit.git"
  },
  "bugs": {
    "url": "https://github.com/saasblast/aurorakit/issues"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "license": "MIT",
  "scripts": {
    "build": "tsup src/index.ts --format=esm,cjs --dts",
    "build-fast": "tsup src/index.ts --format=esm,cjs --no-dts",
    "dev": "pnpm build-fast --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@pandacss/preset-panda": "0.17.1"
  },
  "devDependencies": {
    "@pandacss/dev": "0.17.1",
    "@pandacss/types": "0.17.1"
  },
  "peerDependencies": {
    "@pandacss/dev": "0.17.1"
  }
}
